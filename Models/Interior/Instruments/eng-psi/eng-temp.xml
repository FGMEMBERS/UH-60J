<?xml version="1.0"?>

<PropertyList>

 <path>eng-temp.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>PtrR</object-name>
 <object-name>Case</object-name> 
  <emission>
   <red>0.5</red>
   <green>0.25</green>
   <blue>0.25</blue>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
  </emission>
 </animation>
      
 
 <animation> 
  <type>rotate</type>
  <object-name>PtrR</object-name>
  <property>engines/engine[0]/tot-degc</property>
  <interpolation>
   <entry><ind>0</ind><dep>67</dep></entry>
   <entry><ind>600</ind><dep>0</dep></entry>
   <entry><ind>1200</ind><dep>-67</dep></entry>
  </interpolation>
  <axis> 
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>-0.003</x-m>
   <y-m>0.000</y-m>
   <z-m>-0.002</z-m>  
  </center>
 </animation>         
 
</PropertyList>


